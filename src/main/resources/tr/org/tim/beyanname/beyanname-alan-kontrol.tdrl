package tr.org.tim.beyanname;

rule "beyanname-alan-kontrol_0"
	when 
		beyanname : beyannameDTO(dfifBelge == false)
	then 
		modify( beyanname ) {
			setHata( true )
		}
end
rule "beyanname-alan-kontrol_1"
	when 
		beyanname : beyannameDTO(gtip != null)
	then 
		beyanname.setHata( true );
end
rule "beyanname-alan-kontrol_2"
	when 
		beyanname : beyannameDTO(ihracatNetKgMiktar > 0I)
	then 
		beyanname.setHata( false );
end
